					<div class="box"> </div>
					<div class="box"> <%= link_to(image_tag("sotb_box_logo.png"), "/main/sotb") %></div>
					<div class="box"> </div>
				</div>

				<div id="row_2">
					<div class="box"> </div>
					<div class="box"> </div>
					<div class="box wide"> <%= link_to(image_tag("nest_box_logo.png"), "/main/nest") %> </div>
					<div class="box"> </div>
					<div class="box"> </div>
				</div>

				<div class="row_3">
					<div class="box"> </div>
					<div class="box"> </div>
					<div class="box"> </div>
					<div class="box"> </div>
					<div class="box"> <%= link_to(image_tag("rapgenius_box_logo.png"), "/main/rapgenius") %> </div>
					<div class="box"> </div>
				</div>

			</div>

			<span class="desc">  </span>

			<%= yield %>

		</div>

	</body>
</html>
