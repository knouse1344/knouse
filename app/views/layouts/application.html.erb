<!DOCTYPE html>
<html>
	<head>
	  <title>Bryan Knouse</title>
	  <%= stylesheet_link_tag    "application", :media => "all" %>
	  <%= javascript_include_tag "application" %>
	  <%= csrf_meta_tags %>
	</head>
	<body>
		
		<script type='text/javascript'>
			$(function() {
				$('.kwicks').kwicks({
					size: 175,
					maxSize : 350,
					spacing : 5,
					behavior: 'menu'
				});
			});
		</script>

		<div id="main">

			<span class="title"> BRYAN KNOUSE</span> 
			<span class="desc">  </span>
		
			<div id="nav">
				<ul class='kwicks kwicks-vertical'>
					<li> <%= link_to(image_tag("ur_logo.png"), "/main/rochester") %> </li>
					<li> <%= link_to(image_tag("rapgenius_logo.png"), "/main/rapgenius") %> </li>
					<li> <%= link_to(image_tag("sotb_logo.png"), "/main/sotb") %> </li>
					<li> <%= link_to(image_tag("ur_soccer.png"), "/main/ur_soccer") %> </li>
					<li> <%= link_to(image_tag("nest_logo.png"), "/main/nest") %> </li>
				</ul>
			</div>

			<%= yield %>

		</div>

	</body>
</html>
